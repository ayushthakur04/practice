#include<bits/stdc++.h>
using namespace std;
#define MAX 4
int top=-1;
    class Stack{
    private:
    int st_arr[MAX];


    
    
    public:
    void push(int data);
    void print();
    };
    
    
    void Stack::push(int data){
        if (top==MAX-1){
           std::cout<<"stack overflow !!";
            return;
        }
        
        top=top+1;
        st_arr[top]=data;
    }
    
    void Stack::print(){
        for(int i=top;i>=0;i--){
            cout<<st_arr[i]<<"\n";
        }
    }
    
    int main(){
       Stack st;
       
       
       st.push(2);
       st.push(3);
       st.push(4);
       st.push(5);
       st.push(6);
       st.print();
       
       
       return 0;
    }

