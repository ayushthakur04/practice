#include<bits/stdc++.h>
using namespace std;
 void print(vector<int>a)
 {
     for(int i=0;i<a.size();i++)
     {
         cout<<a[i];
     }
 }
 
 int min_ele(vector<int>a,int i){
     int min_ind=i;
     for(;i<a.size();i++)
     {
         if(a[i]<a[min_ind])
         {
             min_ind=i;
         }
         
     }
     return min_ind;
 }
 
 
void sort(vector<int>&a)
{
    for(int i=0;i<a.size();i++)
    {
        int j=min_ele(a,i);
        swap(a[i],a[j]);
        
    }
}


int main()
{
    vector<int>a={3,1,5,7,2};
    sort(a);
    cout<<"sorted array is : "<<endl;
    print(a);
    
}
